<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pages.popups.ManageTeamPopup">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0" spacing="10.0">
         <children>
            <TableView fx:id="table" prefHeight="230.0" prefWidth="400.0">
               <columns>
                  <TableColumn fx:id="usernameCol" maxWidth="400.0" minWidth="100.0" prefWidth="-1.0" text="Username" />
                  <TableColumn fx:id="phoneNumCol" maxWidth="300.0" minWidth="100.0" prefWidth="-1.0" text="Phone Number" />
                  <TableColumn fx:id="addressCol" maxWidth="400.0" minWidth="80.0" prefWidth="-1.0" text="Address" />
                  <TableColumn fx:id="selectCol" maxWidth="60.0" minWidth="40.0" prefWidth="40.0" text="Select" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <HBox alignment="CENTER" spacing="5.0">
               <children>
                  <ComboBox fx:id="combo" prefWidth="150.0" />
                  <TextField fx:id="textField" promptText="Filter search here!" />
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </HBox>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets bottom="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
