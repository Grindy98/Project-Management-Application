<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>


<AnchorPane prefHeight="40.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Region maxHeight="1.7976931348623157E308" maxWidth="-Infinity" prefWidth="20.0" HBox.hgrow="NEVER" />
            <Label fx:id="nameLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="60.0" text="Project Name" HBox.hgrow="ALWAYS" />
            <Label fx:id="noParticipantsLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="40.0" text="Participants" HBox.hgrow="ALWAYS" />
            <BorderPane maxHeight="1.7976931348623157E308" prefWidth="50.0" HBox.hgrow="ALWAYS">
               <center>
                  <Button fx:id="selectButton" mnemonicParsing="false" text="Select" />
               </center>
            </BorderPane>
            <BorderPane fx:id="deleteBorderPane" layoutX="244.0" layoutY="10.0" maxHeight="1.7976931348623157E308" prefWidth="50.0">
               <center>
                  <Button fx:id="deleteButton" mnemonicParsing="false" style="-fx-background-color: red;" text="Delete" />
               </center>
            </BorderPane>
         </children>
      </HBox>
   </children>
</AnchorPane>
